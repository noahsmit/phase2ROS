<?xml version="1.0" encoding="UTF-8"?>

<behavior name="Surprise Run 1 THOR">

	<executable package_path="vigir_behavior_surprise_run_1_thor.surprise_run_1_thor_sm" class="SurpriseRun1THORSM" />
	<tagstring>task thor</tagstring>
	<author>Philipp Schillinger, Dorothea Koert</author>
	<date>Mon Jun 01 2015</date>
	<description>
		Behavior for the surprise task on run 1 of the DRC Finals.
	</description>


	<!-- Contained Behaviors -->

	<!-- Available Parameters -->
	<params>

		<param type="enum" name="hand_side" default="left" label="Hand Side" hint="Sets the hand_side">
			<option value="left" />
			<option value="right" />
		</param>

		<param type="enum" name="hand_type" default="vt_hand" label="Hand Type" hint="Sets the hand_type">
			<option value="vt_hand" />
			<option value="robotiq" />
		</param>

		<param type="enum" name="parameter_set" default="drc_step_2D" label="Step Parameters" hint="Sets the parameters for footstep planning">
			<option value="drc_step_2D" />
			<option value="drc_step_no_collision" />
		</param>

	</params>


</behavior>